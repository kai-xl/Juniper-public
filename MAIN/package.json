    "tailwindcss": "^3.4.17",
    "typescript": "^5",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4"
  },
  "build": {
    "appId": "com.juniper.app",
    "productName": "JUNIPER",
    "directories": {
      "output": "dist"
    },
    "files": [
      "src/**/*",
      "assets/**/*",
      "config/**/*"
    ],
    "win": {
      "icon": "assets/icon-ultra.png",
      "target": [
        {
          "target": "dir",
          "arch": ["x64"]
